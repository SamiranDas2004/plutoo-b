# OpenAI API Key (for Whisper transcription)
OPENAI_API_KEY=your_openai_key_here

# OpenRouter API Key (for embeddings)
OPENROUTER_API_KEY=your_openrouter_key_here

# Database
DATABASE_URL=mysql+pymysql://root:password@localhost:3306/plutochat

# Pinecone
PINECONE_API_KEY=your_pinecone_key_here
PINECONE_INDEX=plutochat

# Cloudinary (for audio storage)
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# SMTP Email Configuration (for ticket notifications)
# For Gmail: Use your Gmail address and App Password (not regular password)
# Enable 2FA and generate App Password at: https://myaccount.google.com/apppasswords
SMTP_EMAIL=your_email@gmail.com
SMTP_PASSWORD=your_app_password_here
